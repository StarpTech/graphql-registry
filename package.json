{
  "private": true,
  "version": "0.0.0",
  "main": "dist/worker.production.js",
  "description": "Serverless GraphQL registry",
  "license": "MIT",
  "scripts": {
    "dev": "wrangler dev",
    "build": "webpack",
    "deploy": "NODE_ENV=production wrangler publish",
    "test": "npm run unit",
    "unit": "ava src/**/*.test.ts",
    "watch": "npm run unit -- --watch",
    "cov": "nyc --reporter=lcov --reporter=text-summary -- npm run unit",
    "format": "prettier --write '**/*.{js,ts,json,md}'",
    "types": "tsc --noEmit",
    "release": "release-it --no-npm --github.release"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist/worker.production.js"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "nodeArguments": []
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.1",
    "@cloudflare/wrangler": "^1.16.0",
    "ava": "^3.15.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "release-it": "^14.6.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.35.0",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "@apollo/federation": "^0.23.0",
    "@graphql-inspector/core": "^2.5.0",
    "@sindresorhus/fnv1a": "^2.0.1",
    "fast-sort": "^3.0.2",
    "graphql": "^15.5.0",
    "superstruct": "^0.15.1",
    "ulid": "^2.3.0",
    "worktop": "^0.5.0"
  },
  "keywords": [
    "serviceworker",
    "cloudflare",
    "graphql",
    "registry",
    "cache",
    "schema",
    "federation"
  ]
}
